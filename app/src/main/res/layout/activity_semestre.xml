        android:id="@+id/txtSemestre5"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="5° Semestre"
        android:gravity="center"
        android:textColor="@color/fundo_splah"
        android:textSize="22dp"
        android:textStyle="bold"
        android:layout_margin="12dp"
        android:clickable="true"/>
    <TextView
        android:id="@+id/txtSemestre6"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="6° Semestre"
        android:gravity="center"
        android:textColor="@color/fundo_splah"
        android:textSize="22dp"
        android:textStyle="bold"
        android:layout_margin="12dp"
        android:clickable="true"/>
    <TextView
        android:id="@+id/txtSemestre7"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="7° Semestre"
