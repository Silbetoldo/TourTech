        android:layout_margin="12dp"
        android:clickable="true"/>
    <TextView
        android:id="@+id/txtSemestre8"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="8° Semestre"
        android:gravity="center"
        android:textColor="@color/fundo_splah"
        android:textSize="22dp"
        android:textStyle="bold"
        android:layout_margin="12dp"
        android:clickable="true"/>

    <TextView
        android:id="@+id/txtSemestre9"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="9° Semestre"
        android:gravity="center"
        android:textColor="@color/fundo_splah"
        android:textSize="22dp"
        android:textStyle="bold"
        android:layout_margin="12dp"
        android:clickable="true"/>
    <TextView
        android:id="@+id/txtSemestre10"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
